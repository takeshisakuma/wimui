    "format": "prettier --write .",
    "lint": "eslint stories/",
    "lint:fix": "eslint stories/ --fix",
    "stylelint:css": "stylelint \"stories/**/*.css\"",
    "stylelint:scss": "stylelint \"stories/**/*.scss\"",
    "stylelint": "npm run stylelint:css && npm run stylelint:scss",
    "stylelint:fix": "stylelint \"stories/**/*.{css,scss}\" --fix",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static"
  },
