  "scripts": {
    "test": "vitest run --project=storybook",
    "test:watch": "vitest --project=storybook",
    "test:coverage": "vitest run --project=storybook --coverage",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:only": "vitest run --config vite.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write .",
    "lint": "eslint stories/",
    "lint:fix": "eslint stories/ --fix",
    "stylelint:css": "stylelint \"stories/**/*.css\" --allow-empty-input",
    "stylelint:scss": "stylelint \"stories/**/*.scss\"",
    "stylelint": "npm run stylelint:css && npm run stylelint:scss",
    "stylelint:fix": "stylelint \"stories/**/*.{css,scss}\" --fix --allow-empty-input",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static"
  },
